{
    "version": 2,
    "builds": [
      {
        "src": "src/backend.cjs",
        "use": "@now/node"
      },
      {
        "src": "src/public/js/formData.js",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/backend.cjs"
      }
    ]
  }